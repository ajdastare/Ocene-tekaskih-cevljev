
{
    
    "version": "2.0.0",
    "tasks": [
        {
            "label": "run file in python",
            "type": "shell",
            "command": "python",
            "args": [
                "-i",
                {
                    "value": "${file}",
                    "quoting": "escape"
                }
            ],
            "problemMatcher": [
                "$tsc"
            ]
        },
        {
            "label": "OCaml",
            "type": "shell",
            "command": "ocaml",
            "args": [
                "-init",
                {
                    "value": "${file}",
                    "quoting": "escape"
                }
            ]
        }
    ]
}


        
        







